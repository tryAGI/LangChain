<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net4.6.2;netstandard2.0;net6.0;net7.0</TargetFrameworks>
        <NoWarn>$(NoWarn);CA1031</NoWarn>
    </PropertyGroup>

    <PropertyGroup Label="NuGet">
        <Description>LangChain Sources, Documents and Loaders abstractions.</Description>
        <PackageTags>$(PackageTags);abstractions</PackageTags>
    </PropertyGroup>

    <ItemGroup Label="Usings">
        <Using Remove="System.Net.Http" />
    </ItemGroup>

</Project>
