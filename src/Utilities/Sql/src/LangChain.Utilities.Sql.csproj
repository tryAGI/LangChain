<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net4.6.2;netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
        <NoWarn>$(NoWarn);CA1031;CA1845;CA1865</NoWarn>
    </PropertyGroup>

    <PropertyGroup Label="NuGet">
        <Description>LangChain Utilities SqlDatabase.</Description>
        <PackageTags>$(PackageTags);sqldatabase</PackageTags>
    </PropertyGroup>

    <ItemGroup Label="Usings">
        <Using Remove="System.Net.Http" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\..\Core\src\LangChain.Core.csproj" />
    </ItemGroup>

</Project>
